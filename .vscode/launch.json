{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch",
      "request": "launch",
      "runtimeArgs": ["${workspaceFolder}/.vscode/launch.sh"],
      "runtimeExecutable": "bash",
      "cwd": "${fileDirname}",
      "env": {
        "fileExtname": "${fileExtname}",
        "file": "${file}",
        "url": "http://127.0.0.1:3000/${relativeFile}"
      },
      "outputCapture": "std",
      "skipFiles": ["<node_internals>/**"],
      "type": "node"
    }
  ]
}